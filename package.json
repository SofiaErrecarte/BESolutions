    "bcrypt": "^5.0.1",
    "class-transformer": "^0.3.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.1",
    "eslint-plugin-react": "^7.28.0",
    "express": "^4.17.2",
    "joi": "^17.5.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
